<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:AiS.Views"  
                    xmlns:ViewModels="clr-namespace:AiS.ViewModels"
                    xmlns:Converters="clr-namespace:AiS.Views.Converters"
                    xmlns:Validators="clr-namespace:AiS.Views.Validators">
    <Converters:StudyTypeConverter x:Key="StudyTypeConverter"/>
    <Converters:TeacherConverter x:Key="TeacherConverter"/>
    <Converters:OpenWindowsMenuVisibilityConverter x:Key="OpenWindowsMenuVisibilityConverter"/>
    <Converters:StudentConverter x:Key="StudentConverter"/>
    <Converters:IsNotNullConverter x:Key="IsNotNullConverter"/>
    
    <Validators:NameValidationRule x:Key="NameValidationRule"/>
    <Validators:NumberValidationRule x:Key="NumberValidationRule"/>
    <Validators:DateValidationRule x:Key="DateValidationRule"/>
    <Validators:TimeValidationRule x:Key="TimeValidationRule"/>
</ResourceDictionary>