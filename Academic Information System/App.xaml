<Application x:Class="AiS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ViewModels="clr-namespace:AiS.ViewModels"
             xmlns:Views="clr-namespace:AiS.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type ViewModels:TestHomeViewModel}">
            <Views:TestHomeView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type ViewModels:TestWorkViewModel}">
            <Views:TestWorkView />
        </DataTemplate>
    </Application.Resources>
</Application>
